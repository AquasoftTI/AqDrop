<?xml version="1.0" encoding="utf-8" ?>

<codetemplate	xmlns="http://schemas.borland.com/Delphi/2005/codetemplates"
				version="1.0.0">
	<template name="fornbe" invoke="manual">
		<script language="Delphi" onenter="false" onleave="false" onvalidate="true">
			ValidateForTemplate;
		</script>
		<point name="index">
			<text>
			lI
			</text>
			<hint>
				loop counter
			</hint>
		</point>
		<point name="val">
			<text>
			  Low
			</text>
			<hint>
				loop low bound
			</hint>
		</point>
		<point name="count">
			<text>
				High
			</text>
			<hint>
				loop high bound
			</hint>
		</point>
		<description>
			for (no begin/end)
		</description>
		<author>
			Embarcadero
		</author>
		<script language="Delphi" onenter="false" onleave="true">
			DeclareVariable(|index|);
		</script>
		<code language="Delphi" context="methodbody" delimiter="|"><![CDATA[for |index| := |val| to |count| do
|*||end|
]]>
		</code>
	</template>
</codetemplate>
